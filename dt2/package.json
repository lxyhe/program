{
  "name": "lexianyiapp",
  "description": "Lexianyi App",
  "repository": {
    "type": "git",
    "url": "https://github.com/vanishs/dt.git"
  },
  "scripts": {
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve"
  },
  "dependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/compiler-cli": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "2.2.1",
    "@angular/http": "2.2.1",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/platform-server": "2.2.1",
    "@ionic/storage": "1.1.6",
    "ionic-angular": "2.0.0-rc.3-201611211919",
    "ionic-native": "2.2.3",
    "ionicons": "3.0.0",
    "moment": "2.15.1",
    "protobufjs": "5.0.1",
    "qr-image": "^3.1.0",
    "rxjs": "5.0.0-beta.12",
    "umdwebsocket": "^1.0.6",
    "urijs": "^1.18.2",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "@ionic/app-scripts": "0.0.44",
    "typescript": "2.0.10",
    "webpack-bundle-analyzer": "^1.5.3"
  },
  "config": {
    "ionic_bundler": "webpack"
  },
  "cordovaPlugins": [
    "cordova-plugin-inappbrowser",
    {
      "locator": "https://github.com/driftyco/cordova-plugin-wkwebview-engine.git",
      "id": "cordova-plugin-wkwebview-engine"
    },
    "cordova-plugin-whitelist",
    "cordova-plugin-console",
    "cordova-plugin-device",
    "ionic-plugin-keyboard",
    "cordova-plugin-statusbar",
    {
      "locator": "https://github.com/apache/cordova-plugin-splashscreen.git",
      "id": "cordova-plugin-splashscreen"
    },
    {
      "locator": "https://github.com/vanishs/phonegap-plugin-barcodescanner.git",
      "id": "phonegap-plugin-barcodescanner",
      "variables": {
        "CAMERA_USAGE_DESCRIPTION": "Scan QR-Codes"
      }
    },
    {
      "id": "cordova-plugin-wechat",
      "locator": "cordova-plugin-wechat",
      "variables": {
        "WECHATAPPID": "wx45c8d4e24d264ff0"
      }
    },
    "cordova-plugin-geolocation",
    "cordova-plugin-compat",
    "cordova-plugin-crosswalk-webview"
  ],
  "cordovaPlatforms": [
    "ios",
    "android",
    {
      "platform": "android",
      "version": "",
      "locator": "android"
    },
    {
      "platform": "ios",
      "version": "",
      "locator": "ios"
    }
  ]
}
